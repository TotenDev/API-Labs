{
    "base": "http://mywebapp/api",
    "user": "root",
    "password": "123456",
    "modules": [
        {
        "title": "Log Module",
        "methods": [
            {
                "method": "GET",
                "title": "/log",
                "description": "List all log entries"
            },
            {
                "method": "GET",
                "title": "/log/{id}",
                "description": "Find a specific log entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Log entry ID number"
                    }
                ]
            },
            {
                "method": "POST",
                "title": "/log",
                "description": "Create a new log entry",
                "parameters": [
                    {
                        "parameter": "title",
                        "description": "Log entry title"
                    },
                    {
                        "parameter": "date",
                        "description": "Log entry date"
                    },
                    {
                        "parameter": "content",
                        "description": "Log contents"
                    }
                ]
            },
            {
                "method": "PUT",
                "title": "/log",
                "description": "Update an existing log entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Log entry ID number"
                    },
                    {
                        "parameter": "title",
                        "description": "Log entry title"
                    },
                    {
                        "parameter": "date",
                        "description": "Log entry date"
                    },
                    {
                        "parameter": "content",
                        "description": "Log contents"
                    }
                ]
            },
            {
                "method": "DELETE",
                "title": "/log/{id}",
                "description": "Remove an existing log entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Log entry ID number"
                    }
                ]
            }
        ]
        },
