{
    "base": "http://foobar/api",
    "modules": [
        {
        "title": "Foo Module",
        "methods": [
            {
                "method": "GET",
                "title": "/foo",
                "description": "List all foo entries"
            },
            {
                "method": "GET",
                "title": "/foo/{id}",
                "description": "Find a specific foo entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Foo entry ID number"
                    }
                ]
            },
            {
                "method": "POST",
                "title": "/foo",
                "description": "Create a new foo entry",
                "parameters": [
                    {
                        "parameter": "title",
                        "description": "Foo entry title"
                    },
                    {
                        "parameter": "date",
                        "description": "Foo entry date"
                    },
                    {
                        "parameter": "content",
                        "description": "Foo contents"
                    }
                ]
            },
            {
                "method": "PUT",
                "title": "/foo",
                "description": "Update an existing foo entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Foo entry ID number"
                    },
                    {
                        "parameter": "title",
                        "description": "Foo entry title"
                    },
                    {
                        "parameter": "date",
                        "description": "Foo entry date"
                    },
                    {
                        "parameter": "content",
                        "description": "Foo contents"
                    }
                ]
            },
            {
                "method": "DELETE",
                "title": "/foo/{id}",
                "description": "Remove an existing foo entry by ID",
                "parameters": [
                    {
                        "parameter": "id",
                        "description": "Foo entry ID number"
                    }
                ]
            }
        ]
        },
